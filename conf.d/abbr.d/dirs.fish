# Directory listing and movement abbreviations.
abbr -g d ls
abbr -g a "ls -a"
abbr -g da "ls -a"
abbr -g v "ls -l"
abbr -g va "ls -la"
abbr -g up "cd .."
abbr -g ... "cd ../.."
abbr -g .... "cd ../../.."
abbr -g ..... "cd ../../../.."
abbr -g ...... "cd ../../../../.."
abbr -g prev "cd -"

